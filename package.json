{
  "name": "next-tailwind-typescript-starter",
  "version": "1.0.0",
  "scripts": {
    "dev:local": "env-cmd -f environments/.env.local node server.js",
    "dev:stage": "env-cmd -f environments/.env.stage node server.js",
    "dev:pi": "env-cmd -f environments/.env.pi node server.js",
    "dev:prod": "env-cmd -f environments/.env.prod node server.js",
    "start": "next start",
    "stage": "yarn build; yarn start",
    "type-check": "tsc",
    "build:local": "env-cmd -f environments/.env.local next build && next export",
